@model IEnumerable<Admin.Models.TeachersClasses>

<h2>Müəllimlər</h2>
<table id="example" class="table table-striped table-bordered">
    <thead>
        <tr>
            @*<th>İD</th>*@
            @*<th>Müəllim İD</th>*@
            <th>Ad</th>
            <th>Soyad</th>
            <th>Fənn</th>
            @*<th>Sinif İD</th>*@
            <th>Sinif Nömrəsi</th>
        </tr>
    </thead>

    <tfoot>
        <tr>
            @*<th>İD</th>*@
            @*<th>Müəllim İD</th>*@
            <th>Ad</th>
            <th>Soyad</th>
            <th>Fənn</th>
            @*<th>Sinif İD</th>*@
            <th>Sinif Nömrəsi</th>
        </tr>
    </tfoot>

    <tbody>
        @foreach (var teacherclasses in Model)
        {
        <tr>
            @*<td>@teacherclasses.ID</td>
            <td>@teacherclasses.TeacherID</td>*@
            <td>@teacherclasses.Teacher.Name</td>
            <td>@teacherclasses.Teacher.Surname</td>
            <td>@teacherclasses.Teacher.Subject</td>
            @*<td>@teacherclasses.ClassID</td>*@
            <td>@teacherclasses.TableClass.ClassNo</td>
        </tr>
        }
    </tbody>
</table>
