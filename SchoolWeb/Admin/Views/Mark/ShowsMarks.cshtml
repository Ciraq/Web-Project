@model IEnumerable<Admin.Models.Mark>

<h1>Şagirdin Qiyməti</h1>

<table class="table table-bordered table-striped" style="font-size:small">
    <thead>
        <tr>
            <th>Azerbaijani</th>
            <th>Biology</th>
            <th>Chemistry</th>
            <th>English</th>
            <th>Math</th>
            <th>Geography</th>
            <th>History</th>
            <th>Informatics</th>
            <th>Physics</th>
            <th>Literature</th>
            <th>Semester</th>
            <th>Result</th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var mark in Model)
        {
        <tr>
            <td>@mark.Azerbaijani</td>
            <td>@mark.Biology</td>
            <td>@mark.Chemistry</td>
            <td>@mark.English</td>
            <td>@mark.Math</td>
            <td>@mark.Geography</td>
            <td>@mark.History</td>
            <td>@mark.Informatics</td>
            <td>@mark.Physics</td>
            <td>@mark.Literature</td>
            <td>@mark.Semester</td>
            <td>@mark.Result</td>
            <td>
                <a href="@Url.Action("Update","Mark",new {id=mark.ID })" class="btn btn-primary btn-sm" style="color:white"><i class="fa fa-pen fa-sm"></i></a>
            </td>
            <td>
                <a href="@Url.Action("Delete","Mark",new {id=mark.ID })" class="btn btn-danger btn-sm" style="color:white"><i class="fa fa-trash fa-sm"></i></a>
            </td>
        </tr>
        }
    </tbody>
</table>

